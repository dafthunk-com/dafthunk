{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "dafthunk-api-test",
  "compatibility_date": "2024-10-22",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "dafthunk-test",
      "database_id": "test-db-id",
      "preview_database_id": "test-db-id",
      "migrations_dir": "./src/db/migrations"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "test-kv-id"
    }
  ],
  "r2_buckets": [
    {
      "binding": "RESSOURCES",
      "bucket_name": "test-ressources",
      "preview_bucket_name": "test-ressources"
    },
    {
      "binding": "DATASETS",
      "bucket_name": "test-datasets",
      "preview_bucket_name": "test-datasets"
    }
  ],
  "analytics_engine_datasets": [
    { "binding": "COMPUTE", "dataset": "test-compute" }
  ],
  "workflows": [
    {
      "name": "dafthunk-api-test",
      "binding": "EXECUTE",
      "class_name": "Runtime"
    }
  ],
  "unsafe": {
    "bindings": [
      {
        "name": "RATE_LIMIT_DEFAULT",
        "type": "ratelimit",
        "namespace_id": "1001",
        "simple": {
          "limit": 500,
          "period": 60
        }
      },
      {
        "name": "RATE_LIMIT_AUTH",
        "type": "ratelimit",
        "namespace_id": "1002",
        "simple": {
          "limit": 10,
          "period": 60
        }
      },
      {
        "name": "RATE_LIMIT_EXECUTE",
        "type": "ratelimit",
        "namespace_id": "1003",
        "simple": {
          "limit": 30,
          "period": 60
        }
      }
    ]
  },
  "vars": {
    "WEB_HOST": "https://test.dafthunk.com",
    "CLOUDFLARE_ENV": "test",
    "CLOUDFLARE_ACCOUNT_ID": "test-account-id",
    "CLOUDFLARE_API_TOKEN": "test-api-token"
  }
}
